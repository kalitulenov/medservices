generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
  usertype  String
}

model sprmnubar {
  id        Int      @id @default(autoincrement())
  label     String 
  href      String
  typeusr   String
  number    String
}

 model sprusr {
        Id Int    @id @default(autoincrement())
        UsrKod    Int
        UsrOrg    Int
        UsrLog    String
        UsrPsw    String
        UsrTyp    String
        UsrFio    String
        UsrTel    String
   }

model sprcty  {
        Id Int    @id @default(autoincrement())
        CtyKod    Int
        CtyNam    String
        CtyReg    String
    }

model sprusl  {
        Id Int    @id @default(autoincrement())
        UslTrf    String
        UslNam    String
        UslEdn    String
        UslZen    Int
    }


model sprfrmusl  {
        Id Int    @id @default(autoincrement())
        UslFrmHsp Int
        UslFrmTrf String
        UslMinLet Int
        UslMaxLet Int
    }


 model sprorg {
        Id Int    @id @default(autoincrement())
        OrgKod    Int
        OrgCty    Int
        OrgNam    String
        OrgNamShr String
        OrgAdr    String
        OrgTel    String
        OrgDmu    String
    }
