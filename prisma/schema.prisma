generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
  usertype  String
}

model sprmnubar {
  id      Int    @id @default(autoincrement())
  label   String
  href    String
  typeusr String
  number  String
}

model sprusr {
  id     Int    @id @default(autoincrement())
  usrkod Int
  usrorg String
  usrlog String @unique
  usrpsw String
  usrtyp String
  usrfio String
  usrtel String
}

model sprcty {
  id     Int    @id @default(autoincrement())
  ctykod Int
  ctynam String
  ctyreg String
}

model sprusl {
  id     Int     @id @default(autoincrement())
  usltrf String?
  uslnam String?
  usledn String?
  uslzen Int?
}

model spruslfrm {
  id        Int    @id @default(autoincrement())
  uslfrmhsp Int
  uslfrmtrf String?
  uslfrmedn String? 
  uslfrmnam String?
  uslfrmflg Boolean?
  uslminlet Int?
  uslmaxlet Int?
}

model sprorg {
  id        Int    @id @default(autoincrement())
  orgkod    Int
  orgcty    Int
  orgnam    String
  orgnamshr String
  orgadr    String
  orgtel    String
  orgdmu    String
}
